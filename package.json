{
  "name": "rabbiteer-2016",
  "version": "0.0.1",
  "description": "A skeleton for a NodeJS application",
  "keywords": [
    "node project",
    "nodejs project",
    "node seed",
    "eslint",
    "mocha",
    "istanbul",
    "code coverage",
    "gulp",
    "es6",
    "babel"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "./build/coverage"
  },
  "main": "",
  "scripts": {
    "compile": "babel src --out-dir build/source --source-maps",
    "start": "npm run build && node build/source",
    "run": "babel-node src",
    "test": "mocha",
    "coverage": "nyc mocha",
    "lint": "eslint src test www/app",
    "build": "gulp",
    "docs": "jsdoc -d build/jsdoc src",
    "postinstall": "bower install & typings install & cd www & typings install & cd ..",
    "gulp": "gulp",
    "watch": "gulp watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RetroRabbit/rabbiteer-2016"
  },
  "author": "Jade Abbott",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RetroRabbit/rabbiteer-2016"
  },
  "homepage": "https://github.com/RetroRabbit/rabbiteer-2016",
  "devDependencies": {
    "angular": "^1.5.8",
    "angular-loading-bar": "^0.9.0",
    "angular-ui-router": "^0.3.1",
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "bower": "^1.7.9",
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "connect-livereload": "^0.6.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-help": "^1.6.1",
    "gulp-jsdoc3": "^1.0.0",
    "gulp-mocha": "^3.0.1",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "jsdoc": "^3.4.1",
    "mocha": "^3.0.2",
    "node-sass": "^3.10.0",
    "nyc": "^8.3.0",
    "run-sequence": "^1.2.2",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "tiny-lr": "^0.2.1",
    "typings": "^1.4.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "babel-core": "^6.16.0",
    "body-parser": "^1.15.2",
    "compass": "^0.1.1",
    "express": "^4.14.0",
    "express-spa-router": "0.0.8",
    "firebase": "^3.4.1",
    "morgan": "^1.7.0",
    "opn": "^4.0.2"
  }
}
